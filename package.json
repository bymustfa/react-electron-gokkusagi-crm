{
  "name": "gokkusagi-crm",
  "version": "1.0.0",
  "author": "Mustafa ÖZTÜRK",
  "private": true,
  "main": "./public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"SET BROWSER=none&&npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron": "wait-on tcp:3000 && electron .",
    "package": "electron-packager . electron-tutorial-app --overwrite  --platform=win32 --arch=ia32 --icon=public/favicon.ico --prune=true --out=out/windows --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Gökkusağı CRM' ",
    "preelectron-pack": "yarn build",
    "pp": "build --em.main=build/electron.js"
  },
  "dependencies": {
    "@fullcalendar/core": "^5.7.2",
    "@fullcalendar/daygrid": "^5.7.2",
    "@fullcalendar/list": "^5.7.2",
    "@fullcalendar/react": "^5.7.0",
    "@fullcalendar/scrollgrid": "^5.7.2",
    "@fullcalendar/timegrid": "^5.7.2",
    "@reduxjs/toolkit": "^1.6.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@yaireo/tagify": "^4.3.1",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "craco": "^0.0.3",
    "dotenv": "^10.0.0",
    "fuse.js": "^6.4.6",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-detect-click-outside": "^1.1.1",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.3",
    "react-dropzone": "^11.3.2",
    "react-hook-form": "^7.8.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toast-notifications": "^2.4.4",
    "sass": "^1.34.1",
    "sweetalert2": "^11.0.18",
    "web-vitals": "^1.0.1",
    "electron": "^13.1.1",
    "electron-is-dev": "^2.0.0",
    "concurrently": "^6.2.0",
    "electron-packager": "^15.2.0",
    "wait-on": "^5.3.0"
  },
  "devDependencies": {
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
